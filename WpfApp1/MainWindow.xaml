<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Choose file" HorizontalAlignment="Left" Height="47" Margin="28,40,0,0" VerticalAlignment="Top" Width="125" Click="Button_Click"/>
        <DatePicker x:Name="DateFrom" HorizontalAlignment="Left" Margin="10,104,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.322,0.202" SelectedDateChanged="DateFrom_SelectedDateChanged"/>
        <DatePicker x:Name="DateTo" HorizontalAlignment="Left" Margin="143,104,0,0" VerticalAlignment="Top" SelectedDateChanged="DateTo_SelectedDateChanged"/>
        <TextBox x:Name="MostText" HorizontalAlignment="Left" Height="22" Margin="10,161,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="81"/>
        <TextBox x:Name="LeastText" HorizontalAlignment="Left" Height="22" Margin="143,161,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="102"/>
        <ListBox x:Name="ListBoxDraw2" HorizontalAlignment="Left" Height="197" Margin="398,104,0,0" VerticalAlignment="Top" Width="157"/>

    </Grid>
</Window>
